<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatActionButton">
        <attr name="text" format="string"/>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="outsideColor" format="color"/>
        <attr name="insideColor" format="color"/>
        <attr name="borderSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="SwipeItemLayout">
        <!--拖动方向-->
        <attr name="swipeDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <!--显示模式-->
        <attr name="showMode" format="enum">
            <enum name="pull" value="0" />
            <enum name="becover" value="1" />
        </attr>
        <!--滑动距离-->
        <attr name="springDistance" format="dimension" />
        <!--启动滑动-->
        <attr name="enableSlide" format="boolean"/>
        <!--使用透明动画-->
        <attr name="enableAlpha" format="boolean"/>
    </declare-styleable>
</resources>
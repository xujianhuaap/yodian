            <!--android:name=".LauncherActivity"-->
            <!--android:targetActivity="${LAUNCHER_ACTIVITY}">-->
            <!--<intent-filter>-->
                <!--<action android:name="android.intent.action.MAIN" />-->
                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            <!--</intent-filter>-->
        <!--</activity-alias>-->
        <activity
            android:name="maimeng.yodian.app.client.android.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.auth.AuthActivity"/>
        <activity
            android:name=".wxapi.WXEntryActivity"
            android:exported="true" />
        <activity
            android:name=".view.WebLauncher"
            android:exported="true"
            android:label="@string/title_activity_web_launcher" >
